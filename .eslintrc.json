{
    "env": {
        "browser": true,
        "node": true,   
        "es6": true,
        "commonjs": true
       
    },
    "globals": {
        "$": true,
        "require": true,
        "process": true
      },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
    
}
